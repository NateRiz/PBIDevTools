{
	"name": "PowerBI Dev Tools",
	"description": "PowerBI Dev Tools",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": {
			"16": "./icon16.png",
			"32": "./icon32.png",
			"48": "./icon48.png",
			"128": "./icon128.png"
		}
	},
	"background": {
		"all_frames": true,
		"scripts": ["./background.js", "./exportApi.js"],
		"css":["./style.css"]
	},
	"web_accessible_resources": [
		"debug16.png",
		"copy.png",
		"kusto.png",
		"info.png",
		"activities.json",
		"debugWindow.html",
		"Performance.rdl",
		"VERSION.txt"
	],
	"permissions": [
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"*://*.pbidedicated.windows.net/",
		"*://*.pbidedicated.windows-int.net/",
		"*://*.analysis.windows.net/",
		"*://*.analysis-df.windows.net/",
		"*://*.powerbi.com/*",
		"*://*.dc.services.visualstudio.com/v2/track/",
		"*://*.content.powerapps.com/*"
	]
}