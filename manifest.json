{
	"name": "PowerBI Dev Tools",
	"description": "PowerBI Dev Tools",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": {
			"16": "./icon16.png",
			"32": "./icon32.png",
			"48": "./icon48.png",
			"128": "./icon128.png"
		}
	},
	"background": {
		"all_frames": true,
		"scripts": ["./background.js", "./exportApi.js"],
		"css":["./style.css"]
	},
	"web_accessible_resources": [
		"debug16.png",
		"copy.png",
		"kusto.png",
		"info.png",
		"activities.json",
		"debugWindow.html"
	],
	"permissions": [
		"webNavigation",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"<all_urls>"
	]
}